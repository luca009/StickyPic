<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="_PrimaryBGColor">#272727</Color>
    <Color x:Key="_SecondaryBGColor">#161616</Color>
    <Color x:Key="_HoverBGColor">Gray</Color>
    <Color x:Key="_SelectedBGColor">#494949</Color>
    <Color x:Key="_FGColor">White</Color>
    <Color x:Key="_DisabledFGColor">#757575</Color>
    <Color x:Key="_BorderColor">#4B4B4B</Color>

    <SolidColorBrush x:Key="PrimaryBGColor" Color="{StaticResource _PrimaryBGColor}" />
    <SolidColorBrush x:Key="SecondaryBGColor" Color="{StaticResource _SecondaryBGColor}" />
    <SolidColorBrush x:Key="HoverBGColor" Color="{StaticResource _HoverBGColor}" />
    <SolidColorBrush x:Key="SelectedBGColor" Color="{StaticResource _SelectedBGColor}" />
    <SolidColorBrush x:Key="FGColor" Color="{StaticResource _FGColor}" />
    <SolidColorBrush x:Key="DisabledFGColor" Color="{StaticResource _DisabledFGColor}" />
    <SolidColorBrush x:Key="BorderColor" Color="{StaticResource _BorderColor}" />

    <Thickness x:Key="RegularThickness" Left="1" Right="1" Top="1" Bottom="1" />
    <Thickness x:Key="PronouncedThickness" Left="2" Right="2" Top="2" Bottom="2" />
</ResourceDictionary>